<Window x:Class="PLWPF.CONTRACT.ListOfContract"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF.CONTRACT"
        mc:Ignorable="d"
        Title="ListOfContract" Height="303.383" Width="561.654">
    <ListView Name="ListOfContracts" Height="230" Margin="0,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="560" RenderTransformOrigin="0.498,0.56">
        <ListView.View>
            <GridView>
                <GridViewColumn Header="Contract ID: " Width="auto" DisplayMemberBinding="{Binding ContractID}" />
                <GridViewColumn Header="Mother ID" Width="auto" DisplayMemberBinding="{Binding MotherID}" />
                <GridViewColumn Header="Nanny ID" Width="auto" DisplayMemberBinding="{Binding BabySitterID}" />
                <GridViewColumn Header="Child ID" Width="auto" DisplayMemberBinding="{Binding ChildID}" />
                <GridViewColumn Header="First Meeting has done?" Width="auto" DisplayMemberBinding="{Binding FirsMeating}" />
            </GridView>
        </ListView.View>
    </ListView>
</Window>
